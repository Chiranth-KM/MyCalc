<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#B2F5E8"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:paddingHorizontal="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="53dp"
            android:layout_height="47dp"
            android:layout_marginStart="4dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/textView7"
            app:layout_constraintHorizontal_bias="0.066"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@drawable/icon" />

        <Button
            android:id="@+id/button5"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/history"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.571" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="56dp"
            android:layout_height="25dp"
            android:layout_marginEnd="176dp"
            android:fontFamily="@font/exo"
            android:text="MyCalc"
            android:textStyle="bold"
            style="@style/Text"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/button5"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.727" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="@drawable/round_text_box"
            android:fontFamily="@font/exo"
            android:gravity="bottom|right"
            android:hint="0"
            android:paddingRight="10dp"
            android:textColor="#F3EEEE"
            android:textColorHint="#EFEDED"
            android:textSize="34sp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="@drawable/round_text_box"
            android:fontFamily="@font/exo"
            android:gravity="bottom|end"
            android:paddingRight="10dp"
            android:textColor="#F3EEEE"
            android:textSize="24sp" />
    </LinearLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3"
        app:layout_constraintVertical_bias="0.0">

        <androidx.gridlayout.widget.GridLayout
            android:id="@+id/gridLayout"
            android:layout_width="332dp"
            android:layout_height="412dp"
            android:transitionName="activity_transition"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <Button
                android:id="@+id/button"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_margin="1dp"
                android:background="@drawable/pink"
                android:fontFamily="@font/exo"
                android:onClick="onclick"
                android:text="C"
                android:textSize="24sp"
                style="@style/Text"
                app:layout_column="0"
                app:layout_row="4" />

            <Button
                android:id="@+id/button63"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_margin="1dp"
                android:background="@drawable/pink"
                android:fontFamily="@font/exo"
                android:onClick="onclick"
                android:text="1"
                android:textSize="24sp"
                app:layout_column="0"
                style="@style/Text"
                app:layout_row="3" />

            <Button
                android:id="@+id/button64"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_margin="1dp"
                android:background="@drawable/dark_pink"
                android:fontFamily="@font/exo"
                android:text="="
                android:textSize="24sp"
                app:layout_column="3"
                style="@style/Text"
                app:layout_row="4" />

            <Button
                android:id="@+id/button65"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_margin="1dp"
                android:background="@drawable/pink"
                android:fontFamily="@font/exo"
                android:onClick="onclick"
                android:text="."
                style="@style/Text"
                android:textSize="24sp"
                app:layout_column="2"
                app:layout_row="4" />

            <Button
                android:id="@+id/button29"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_margin="1dp"
                android:background="@drawable/pink"
                android:fontFamily="@font/exo"
                android:text="AC"
                style="@style/Text"
                android:textSize="24sp"
                app:layout_column="0"
                app:layout_row="0" />

            <Button
                android:id="@+id/button30"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_margin="1dp"
                android:background="@drawable/dark_blue"
                android:fontFamily="@font/exo"
                android:onClick="onclick"
                android:text="("
                android:textSize="24sp"
                app:layout_column="1"
                style="@style/Text"
                app:layout_row="0" />

            <Button
                android:id="@+id/button31"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_margin="1dp"
                android:background="@drawable/dark_blue"
                android:fontFamily="@font/exo"
                android:onClick="onclick"
                android:text=")"
                android:textSize="24sp"
                app:layout_column="2"
                style="@style/Text"
                app:layout_row="0" />

            <Button
                android:id="@+id/button32"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_margin="1dp"
                android:background="@drawable/dark_blue"
                android:fontFamily="@font/exo"
                android:onClick="onclick"
                android:text="/"
                android:textSize="24sp"
                style="@style/Text"
                app:layout_column="3"
                app:layout_row="0" />

            <Button
                android:id="@+id/button33"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_margin="1dp"
                android:background="@drawable/pink"
                android:fontFamily="@font/exo"
                android:onClick="onclick"
                android:text="7"
                android:textSize="24sp"
                style="@style/Text"
                app:layout_column="0"
                app:layout_row="1" />

            <Button
                android:id="@+id/button34"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_margin="1dp"
                android:background="@drawable/pink"
                android:fontFamily="@font/exo"
                android:onClick="onclick"
                android:text="8"
                android:textSize="24sp"
                style="@style/Text"
                app:layout_column="1"
                app:layout_row="1" />

            <Button
                android:id="@+id/button35"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_margin="1dp"
                android:background="@drawable/pink"
                android:fontFamily="@font/exo"
                android:onClick="onclick"
                android:text="9"
                android:textSize="24sp"
                app:layout_column="2"
                style="@style/Text"
                app:layout_row="1" />

            <Button
                android:id="@+id/button36"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_margin="1dp"
                android:background="@drawable/dark_blue"
                android:fontFamily="@font/exo"
                android:onClick="onclick"
                android:text="*"
                android:textSize="24sp"
                app:layout_column="3"
                style="@style/Text"
                app:layout_row="1" />

            <Button
                android:id="@+id/button37"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_margin="1dp"
                android:background="@drawable/pink"
                android:fontFamily="@font/exo"
                android:onClick="onclick"
                android:text="4"
                android:textSize="24sp"
                style="@style/Text"
                app:layout_column="0"
                app:layout_row="2" />

            <Button
                android:id="@+id/button38"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_margin="1dp"
                android:background="@drawable/pink"
                android:fontFamily="@font/exo"
                style="@style/Text"
                android:onClick="onclick"
                android:text="5"
                android:textSize="24sp"
                app:layout_column="1"
                app:layout_row="2" />

            <Button
                android:id="@+id/button39"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_margin="1dp"
                android:background="@drawable/pink"
                android:fontFamily="@font/exo"
                android:onClick="onclick"
                android:text="6"
                android:textSize="24sp"
                style="@style/Text"
                app:layout_column="2"
                app:layout_row="2" />

            <Button
                android:id="@+id/button40"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_margin="1dp"
                android:background="@drawable/dark_blue"
                android:fontFamily="@font/exo"
                android:onClick="onclick"
                android:text="-"
                android:textSize="24sp"
                style="@style/Text"
                app:layout_column="3"
                app:layout_row="2" />

            <Button
                android:id="@+id/button41"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_margin="1dp"
                android:background="@drawable/pink"
                android:fontFamily="@font/exo"
                android:onClick="onclick"
                android:text="0"
                android:textSize="24sp"
                style="@style/Text"
                app:layout_column="1"
                app:layout_row="4" />

            <Button
                android:id="@+id/button42"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_margin="1dp"
                android:background="@drawable/pink"
                android:fontFamily="@font/exo"
                android:onClick="onclick"
                style="@style/Text"
                android:text="2"
                android:textSize="24sp"
                app:layout_column="1"
                app:layout_row="3" />

            <Button
                android:id="@+id/button43"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_margin="1dp"
                android:background="@drawable/pink"
                android:fontFamily="@font/exo"
                android:onClick="onclick"
                android:text="3"
                android:textSize="24sp"
                app:layout_column="2"
                style="@style/Text"
                app:layout_row="3" />

            <Button
                android:id="@+id/button44"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_margin="1dp"
                android:background="@drawable/dark_blue"
                android:fontFamily="@font/exo"
                android:onClick="onclick"
                android:text="+"
                style="@style/Text"
                android:textSize="24sp"
                app:layout_column="3"
                app:layout_row="3" />

        </androidx.gridlayout.widget.GridLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>